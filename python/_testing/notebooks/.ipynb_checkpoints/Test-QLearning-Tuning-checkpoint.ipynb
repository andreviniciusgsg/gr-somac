    "def calc_reward(curr, prev):\n",
    "    if curr > prev:\n",
    "        reward = curr / prev - 1. if prev > 0. else 0.\n",
    "    else:\n",
    "        reward = - (prev / curr - 1.) if curr > 0. else 0.\n",
    "\n",
    "    if reward > 1. or reward < -1:\n",
    "        reward = 1 if reward > 1 else -1\n",
    "\n",
    "    return reward * 5.\n",
    "\n",
    "def run_boltz(init_prot = 0, learn_rate = 0.3, discount = 0.8, dic = {}, T = 1.0, t_csma = [], t_tdma = []):\n",
    "    somac = boltz(prot = init_prot, learn_rate = learn_rate, discount = discount, T = T)\n",
    "    backlog, metric = test(\n",
    "        init_prot = init_prot, learn_rate = learn_rate, discount = discount, dic = dic, alg = somac,\\\n",
    "        t_csma = t_csma, t_tdma = t_tdma\n",
    "    )\n",
    "    return backlog, metric\n",
    "\n",
